{
    "version": 2,
    "env": {
        "BASE_URL_JAKNOT": "@base_url_jaknot"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        }
    ]
}
